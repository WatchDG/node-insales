{
  "name": "node-insales",
  "version": "4.6.0",
  "description": "insales",
  "keywords": [
    "insales"
  ],
  "license": "MIT",
  "author": {
    "name": "Grigoriy Baranov",
    "email": "baranov@w3m.co"
  },
  "repository": "github:WatchDG/node-insales",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "formatter": "npx prettier --write \"src/**/*.ts\"",
    "linter": "npx tslint -p tsconfig.json \"src/**/*.ts\"",
    "build": "npm run formatter && npm run linter && npx tsc",
    "preversion": "npm run build",
    "version": "npm run build",
    "prepublishOnly": "npm run build",
    "prepare": "npm run build",
    "jsdoc": "npm run build && npx jsdoc -d jsdoc lib\\index.js",
    "readme": "npm run build && npx jsdoc2md lib\\index.js > README.md"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "axios": "^0.19.2",
    "node-result": "^5.2.0"
  },
  "devDependencies": {
    "@types/node": "^14.0.23",
    "jsdoc": "^3.6.5",
    "jsdoc-to-markdown": "^6.0.1",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.5"
  }
}
